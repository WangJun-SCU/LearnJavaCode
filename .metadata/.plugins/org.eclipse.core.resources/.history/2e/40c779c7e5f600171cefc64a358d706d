
import java.lang.reflect.Field;
import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) throws IllegalArgumentException, IllegalAccessException {
		// TODO Auto-generated method stub
		String str = "helloWorld";
		System.out.println(str);
		Main test = new Main();
		test.createData();
		
		
		
	}
	
	public void createData() throws IllegalArgumentException, IllegalAccessException {
		List<Model> list = new ArrayList<Model>();
		list.add(new Model("wangjun",23,"men"));
		list.add(new Model("Luyijia",22,"women"));
		for (int i = 0; i < list.size(); i++) {
			Field[] fields = list.get(i).getClass().getDeclaredFields();
			System.out.println(fields.length);
			for(int j = 0; j < fields.length; j++) {
				Field fieldItem = fields[j];
				fieldItem.setAccessible(true); 
				System.out.println(fieldItem.getName() + ":" + fieldItem.get(list.get(i)));
			}
		}
	}
	
	

}
class Model{
	private String name;
	private int age;
	private String sex;
	public Model(String name, int age, String sex) {
		super();
		this.name = name;
		this.age = age;
		this.sex = sex;
	}
}

