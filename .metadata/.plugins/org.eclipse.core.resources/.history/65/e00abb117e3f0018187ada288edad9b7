/*
 * 实现线程的三种方式：
 * 1. 继承Thread
 * 2. 实现Runnable接口
 * 3. 实现Callable接口
 */
public class ThreeWayOfThread {

	public static void main(String[] args) {
		
	}

}
//继承Thread
class Thread1 extends Thread {
	private int n = 5;
	@Override
	public void run() {
		for(int i = 0; i < n; i++) {
			
		}
	}
}

