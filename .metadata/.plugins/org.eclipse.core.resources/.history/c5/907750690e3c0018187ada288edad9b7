package com.wangjun.arithmetic;

public class ReverseArray {

	public static void main(String[] args) {
		
	}
	
	public void reverseArray(int[] arr) {
		int start = 0;
		int end = arr.length - 1;
		while(start < end) {
			int tem = arr[start];
			arr[start] = arr[end];
			arr[end] = tem;
			start++;
			end--;
		}
	}

}
