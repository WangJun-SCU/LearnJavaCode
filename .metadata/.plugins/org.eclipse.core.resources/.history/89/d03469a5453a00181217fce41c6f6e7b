package com.wangjun.othersOfJava;

import java.util.Iterator;
import java.util.TreeSet;

/** 
 * TreeSet的增删遍历
 * @author wangjun 
 * @email  scuwangjun@hotmail.com
 * @time   2018年4月7日 下午4:36:02 
 */
public class TreeSetOperation {

	public static void main(String[] args) {
		TreeSet<String> set = new TreeSet<String>();
		//增
		set.add("key1");
		set.add("key2");
		set.add("key3");
		//删
		set.remove("key1");
		//遍历1,使用Iterator
		//使用set.descendingIterator()方法可以反向遍历
		System.out.println("TreeSet遍历1,使用Iterator:");
		Iterator<String> iterator = set.iterator();
		while(iterator.hasNext()) {
			System.out.println(iterator.next());
		}
		//遍历2，使用for
		System.out.println("TreeSet遍历2，使用for:");
		for(String str: set) {
			System.out.println(str);
		}
	}

}
