package com.wangjun.arithmetic;

/*
 * 插入排序
 */
public class SortInsert {

	public static void main(String[] args) {
		
	}
	
	public void sortInsert(int[] arr) {
		int len = arr.length;
		for(int i = 1; i < len; i++) {
			int j = i;
			while((j > 0) && (arr[j] < arr[j - 1])) {
				int tem = arr[j];
				arr[j] = arr[j - 1];
				arr[j - 1] = tem;
				j--;
			}
		}
	}

}
